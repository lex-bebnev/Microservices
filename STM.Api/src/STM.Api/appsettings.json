{
  "app": {
    "name": "api"
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "api",
    "udpHost": "localhost",
    "udpPort": 6831,
    "maxPacketSize": 0,
    "sampler": "const"
  },
  "rabbitMq": {
    "namespace": "api",
    "retries": 3,
    "retryInterval": 2,
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "localhost"
    ],
    "requestTimeout": "00:00:10",
    "publishConfirmTimeout": "00:00:01",
    "recoveryInterval": "00:00:10",
    "persistentDeliveryMode": true,
    "autoCloseConnection": true,
    "automaticRecovery": true,
    "topologyRecovery": true,
    "exchange": {
      "durable": true,
      "autoDelete": false,
      "type": "Topic"
    },
    "queue": {
      "autoDelete": false,
      "durable": true,
      "exclusive": false
    }
  },
  "swagger": {
    "enabled": true,
    "reDocEnabled": false,
    "name": "v2",
    "title": "STM",
    "version": "v2",
    "routePrefix": "docs",
    "includeSecurity": true
  },
  "AllowedHosts": "*"
}
